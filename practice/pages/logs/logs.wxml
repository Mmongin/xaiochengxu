<!--index.wxml-->
<!--背景-->
<view id="body">
  <view class="centerDemo">
    <view class="detailDemo">
      <text class="huanyin">欢迎</text>
      <view class="information">
        <!--姓名-->
        <view class="infor">
          <image src="../../image/xingming.png"  />
          <input placeholder="请输入姓名" class="name"></input>
        </view> 
        <!--学院-->
        <view class="choose">
          <text class="tip">学院</text>
          <view class="infor">
            <image src="../../image/xueyuan.png"  ></image>
            <text class="name" style="flex:7">请选择学院</text>
            <image src="../../image/sanjiao.png" ></image>
          </view>
        </view>
        <!--班级-->
        <view class="choose">
          <text class="tip">班级</text>
          <view class="infor">
            <image src="../../image/class.png"  ></image>
            <text class="name" style="flex:7">请选择班级</text>
            <image src="../../image/sanjiao.png" ></image>
          </view>
        </view>
        <!--手机-->
        <view class="choose">
          <text class="tip">手机号</text>
          <view class="infor">
            <image src="../../image/phone.png"  ></image>
          <input placeholder="请输入手机号" class="name"></input>
          </view>
        </view>
      </view>
    </view>
    <button class="enter">完成</button>
  </view>
</view>